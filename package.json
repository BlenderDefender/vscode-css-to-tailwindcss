{
  "name": "vscode-css-to-tailwindcss",
  "displayName": "CSS to TailwindCSS converter",
  "description": "CSS to TailwindCSS 3.x converter extension for Visual Studio Code",
  "author": "Salavat Salakhutdinov <salahutdinov.salavat@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "homepage": "https://github.com/jackardios/vscode-css-to-tailwindcss",
  "bugs": {
    "url": "https://github.com/jackardios/vscode-css-to-tailwindcss/issues",
    "email": "hello@bradley.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jackardios/vscode-css-to-tailwindcss.git"
  },
  "publisher": "Jackardios",
  "keywords": [
    "convert",
    "converter",
    "conversion",
    "css",
    "postcss",
    "tailwind",
    "tailwindcss",
    "css-to-tailwind",
    "vscode"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-to-tailwindcss.cssToTailwindCSS",
        "title": "Convert CSS to TailwindCSS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/micromatch": "^4.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "css-to-tailwindcss": "^0.2.5",
    "micromatch": "^4.0.5",
    "postcss-nested": "^6.0.0"
  }
}
